@inreference{DadarParsiColony2022,
  title = {Dadar {{Parsi Colony}}},
  booktitle = {Wikipedia},
  date = {2022-12-05T13:11:14Z},
  url = {https://en.wikipedia.org/w/index.php?title=Dadar_Parsi_Colony&oldid=1125715992},
  urldate = {2023-02-19},
  abstract = {Dadar Parsi Colony (officially Mancherji Joshi Parsi Colony) is an upper class Parsi colony in midtown South Mumbai. It is situated in the locality of Dadar-Matunga. Unlike the other Parsi colonies (also known as 'baugs', literally gardens) it is not surrounded by a wall or fence and is not isolated from its surroundings. The colony houses the famous Five Gardens created by Mancherji Joshi, a renowned Parsi. It is the largest Zoroastrian enclave in the world and in 2009, out of the 45,000 Zoroastrians living in Mumbai, 10,000 lived in Dadar Parsi Colony.},
  langid = {english},
  annotation = {Page Version ID: 1125715992}
}

